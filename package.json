{
  "name": "markdown-translator",
  "version": "1.0.0",
  "description": "A command line tool to translate markdown and MDX files using Google Gemini AI",
  "type": "module",
  "main": "index.js",
  "bin": {
    "md-translate": "bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "demo": "node bin/cli.js translate -i examples/StarRocksTest.md -l \"Simplified Chinese\"",
    "setup": "node bin/cli.js setup",
    "languages": "node bin/cli.js languages",
    "lint": "eslint bin/**/*.js src/**/*.js",
    "test": "rm -f examples/StarRocksTest_simplified_chinese.md && node bin/cli.js translate -i examples/StarRocksTest.md -l \"Simplified Chinese\""
  },
  "keywords": [
    "markdown",
    "docusaurus",
    "translation",
    "gemini",
    "ai",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@playcanvas/eslint-config": "2.1.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "eslint": "^9.31.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "ora": "^8.2.0",
    "path": "^0.12.7"
  },
  "publishConfig": {
    "@StarRocks:registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
